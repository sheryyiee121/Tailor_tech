{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/search.js": {
            "maxDuration": 10
        }
    },
    "env": {
        "GOOGLE_API_KEY": "@google_api_key",
        "GOOGLE_CX": "@google_cx",
        "SCRAPINGDOG_API_KEY": "@scrapingdog_api_key"
    }
}